__xx_get_aws_mq_brokers() {
    if [[ -n "${AWS_SESSION_TOKEN}" && -n "${AWS_VAULT}" ]]; then
        local SQLITE_DATABASE_PATH="${XX_CACHE_DIR}/aws_mq.db"
        local ACCOUNT="${AWS_VAULT}"
        local REGION="${AWS_REGION}"

        if [[ -f "${SQLITE_DATABASE_PATH}" ]]; then
            local CACHE_TIMESTAMP=$(sqlite3 -column "${SQLITE_DATABASE_PATH}" "SELECT sync_timestamp FROM SyncLog WHERE account = '${ACCOUNT}' AND region = '${REGION}';" 2>/dev/null)
            if [[ -n "${CACHE_TIMESTAMP}" ]]; then
                echo "Last cached: ${CACHE_TIMESTAMP}"
                (
                    echo "BROKER_ID^BROKER_NAME^STATE^ENGINE^VERSION^DEPLOYMENT_MODE^INSTANCE_TYPE"
                    sqlite3 "${SQLITE_DATABASE_PATH}" "SELECT broker_id, broker_name, broker_state, engine_type, engine_version, deployment_mode, instance_type FROM Brokers WHERE account = '${ACCOUNT}' AND region = '${REGION}';" | \
                        awk -F'|' '{
                            broker_id = $1
                            broker_name = $2
                            broker_state = $3
                            engine_type = $4
                            engine_version = $5
                            deployment_mode = $6
                            instance_type = $7
                            printf "%s^%s^%s^%s^%s^%s^%s\n", broker_id, broker_name, broker_state, engine_type, engine_version, deployment_mode, instance_type
                        }'
                ) | __xx_format_columns_ansi "^"
                return
            fi
        fi

        echo "No cache found. Run '__xx_cache_aws_mq_brokers_to_sqlite' to cache MQ brokers."
    else
        echo "AWS vault is not enabled."
    fi
}
