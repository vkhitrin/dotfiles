__xx_get_aws_elasticache_clusters() {
    if [[ -n "${AWS_SESSION_TOKEN}" && -n "${AWS_VAULT}" ]]; then
        local SQLITE_DATABASE_PATH="${XX_CACHE_DIR}/aws_elasticache.db"
        local ACCOUNT="${AWS_VAULT}"
        local REGION="${AWS_REGION}"

        if [[ -f "${SQLITE_DATABASE_PATH}" ]]; then
            local CACHE_TIMESTAMP=$(sqlite3 -column "${SQLITE_DATABASE_PATH}" "SELECT sync_timestamp FROM SyncLog WHERE account = '${ACCOUNT}' AND region = '${REGION}';" 2>/dev/null)
            if [[ -n "${CACHE_TIMESTAMP}" ]]; then
                echo "Last cached: ${CACHE_TIMESTAMP}"
                (
                    echo "CLUSTER_ID^ENGINE^VERSION^NODE_TYPE^STATUS^NODES"
                    sqlite3 "${SQLITE_DATABASE_PATH}" "SELECT cluster_id, engine, engine_version, node_type, cluster_status, num_nodes FROM Clusters WHERE account = '${ACCOUNT}' AND region = '${REGION}';" | \
                        awk -F'|' '{
                            cluster_id = $1
                            engine = $2
                            engine_version = $3
                            node_type = $4
                            cluster_status = $5
                            num_nodes = $6
                            printf "%s^%s^%s^%s^%s^%s\n", cluster_id, engine, engine_version, node_type, cluster_status, num_nodes
                        }'
                ) | __xx_format_columns_ansi "^"
                return
            fi
        fi

        echo "No cache found. Run '__xx_cache_aws_elasticache_clusters_to_sqlite' to cache ElastiCache clusters."
    else
        echo "AWS vault is not enabled."
    fi
}
